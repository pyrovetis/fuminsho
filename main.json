{
  "$schema": "https://schema.railpack.com",
  "provider": "python",
  "steps": {
    "build": {
      "commands": [
        "ls -la",
        "python manage.py compress",
        "python manage.py collectstatic --noinput"
      ]
    }
  },
  "deploy": {
    "startCommand": "gunicorn -b 0.0.0.0:8000 -w 2 --log-file - --log-level debug fuminsho.wsgi"
  }
}
